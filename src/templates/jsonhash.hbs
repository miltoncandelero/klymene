{
  "frames": {
    {{#each rects}}
    "{{{name}}}": {
      "frame": {
        "x": {{frame.x}},
        "y": {{frame.y}},
        "w": {{frame.w}},
        "h": {{frame.h}}
      },
      "rotated": {{rotated}},
      "trimmed": {{trimmed}},
      "spriteSourceSize": {
        "x": {{spriteSourceSize.x}},
        "y": {{spriteSourceSize.y}},
        "w": {{spriteSourceSize.w}},
        "h": {{spriteSourceSize.h}}
      },
      "sourceSize": {
        "w": {{sourceSize.w}},
        "h": {{sourceSize.h}}
      },
      "pivot": {
        "x": 0.5,
        "y": 0.5
      }
    }{{#unless @last}},{{/unless}}
    {{/each}}
  },
  "meta": {
    "app": "{{{metadata.url}}}",
    "version": "{{metadata.version}}",
    "image": "{{{metadata.image}}}",
    "format": "{{metadata.format}}",
    "size": {
      "w": {{metadata.size.w}},
      "h": {{metadata.size.h}}
    },
    {{#if config.relatedMultiPacks.lenght}}
    "related_multi_packs":[
      {{#each config.relatedMultiPacks}}
        "{{name}}"{{#unless @last}},{{/unless}}
      {{/each}}
    ],
    {{/if}}
    "scale": {{config.scale}}
  }
}