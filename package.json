{
  "name": "clymene",
  "version": "1.0.0",
  "description": "Mother of (texure) Atlas",
  "type": "module",
  "main": "./dist/tweedle.js",
  "module": "./dist/tweedle.es.js",
  "bundle": "./dist/tweedle.umd.js",
  "types": "./index.d.ts",
  "files": [
    "dist/",
    "index.d.ts",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/miltoncandelero/tweedle.js.git"
  },
  "bugs": {
    "url": "https://github.com/miltoncandelero/tweedle.js/issues"
  },
  "homepage": "https://miltoncandelero.github.io/tweedle.js/",
  "keywords": [
    "tween",
    "tweening",
    "videogames",
    "typescript",
    "animation"
  ],
  "author": "Milton Candelero <miltoncandelero@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@killabunnies/eslint-config": "^1.0.5",
    "@microsoft/api-extractor": "^7.33.7",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-sucrase": "^5.0.1",
    "@types/jest": "^29.2.4",
    "@types/node": "*",
    "@types/sharp": "^0.31.1",
    "eslint": "^8.30.0",
    "eslint-plugin-jest": "^27.1.7",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.23",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "start": "ts-node ./src/index.mts",
    "lint": "run-p lint:*",
    "lint:src": "eslint src/**/*.ts --fix",
    "lint:test": "eslint test/**/*.ts --fix",
    "build": "run-s bundle types",
    "bundle": "tsc --outDir dist --project tsconfig.json",
    "prebuild": "run-s clean:dist clean:types",
    "clean": "run-p clean:*",
    "clean:dist": "rimraf ./dist",
    "clean:types": "rimraf ./compile",
    "clean:docs": "rimraf ./docs",
    "types": "tsc --outDir compile --project tsconfig.types.json && api-extractor run && rimraf compile",
    "prepublishOnly": "run-s test build doc",
    "test": "jest",
    "doc": "typedoc"
  },
  "dependencies": {
    "maxrects-packer": "^2.7.3",
    "sharp": "^0.31.3"
  }
}
